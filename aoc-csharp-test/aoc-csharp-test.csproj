    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\aoc-csharp-utils\aoc-csharp-utils.csproj" />
      <ProjectReference Include="..\aoc-csharp\aoc-csharp.csproj" />
    </ItemGroup>

